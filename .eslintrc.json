{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "import/extensions": [2, "ignorePackages"],
        "prefer-destructuring": ["error", {"object": true, "array": false}],
        "import/no-unresolved": [0],
        "import/no-extraneous-dependencies": [0]
    }
}
